{
  "manifest_version": 3,
  "name": "CommuniteeGolf Chat",
  "version": "1.0.0",
  "description": "A chat extension for CommuniteeGolf",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}